[code]
if(@ARGV &lt; 3) {
usage();
}

open(INFO, @ARGV[0]);
	@lines = &lt;INFO&gt;;
close(INFO);

print &quot;File: &quot; . @ARGV[0] . &quot;\n&quot;;
print &quot;Start Line: &quot; . @ARGV[1] . &quot;\n&quot;;
print &quot;End Line: &quot; . @ARGV[2] . &quot;\n\n&quot;;

#BUG FIXED: Prints line 6 :)
for(($i=@ARGV[1]-1); $i&lt;@ARGV[2]; $i++) {
	print $i + 1 . &#39;:  &#39; . @lines[($i)];
}

sub usage() {
	print &quot;\nProper Usage:\n&quot;;
	print &quot;file_reader.pl -filename -startLine -endLine\n&quot;;
	exit();
}
[/code]</description>
    
