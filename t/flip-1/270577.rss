              <pre><code class="lang-auto">&lt;?php

  $text = "abba";

  $old = array("a",        "b", "c");
  $new = array("ɐ", "q", "&amp;#x0254");

  if($_REQUEST['flip'] == 1) $text = str_replace($old, $new, $text);

  echo $text;

?&gt;</code></pre>
