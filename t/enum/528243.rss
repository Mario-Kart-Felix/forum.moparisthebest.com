          <blockquote>
              <p>[code= java]package server.model.players.skills;</p>
<p>import server.Config;<br>
import server.model.players.Client;</p>
<p>public class BoneBurying {</p>
<pre><code>/*
 * Buries the item if the item clicked is in the values of the enum,
 * if not it will return.
 */
public static void buryBone(Client c, int itemId) {
	for (bonesData e : bonesData.values()) {
		if (itemId == e.getId()) {
			c.getItems().deleteItem(e.getId(), 1);
			c.sendMessage("You bury a "+ bonesData  +" bone.");
			c.getPA().addSkillXP(e.getExperience(), 5);
			c.startAnimation(827);
		}
	}
}

private enum bonesData {

	NORMAL(2, 2);

	private int id, experience;

	public int getId() {
		return id;
	}

	public int getExperience() {
		return experience;
	}

	private bonesData(int id, int experience) {
		this.id = id;
		this.experience = experience;
	}
}
</code></pre>
