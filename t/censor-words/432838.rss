        <dc:creator><![CDATA[@EclipseRSPS EclipseRSPS]]></dc:creator>
        <description><![CDATA[
          <p><a href="https://forum.moparisthebest.com/u/eclipsersps">@EclipseRSPS</a> wrote:</p>
          <blockquote>
              <p>If your not retarded you can convert this.</p>
<p>[code]package com.rs.game.player.content;</p>
<p>import com.rs.game.player.Player;</p>
<p>public class Censor {</p>
<pre><code>public static String checkMessage(Player player, String msg) {
	String[] rudeWords = { "" };
	String[] advertiseStrings = { "" };
	for (int i = 0; i &lt; rudeWords.length; i++) {
		if (msg.contains(rudeWords[i])) {
			msg = msg
					.replace(rudeWords[i], getStars(rudeWords[i].length()));
		} else
			if (player.getRights() &gt;= 0)
				if (msg.contains(rudeWords[i])) {
					return null;

				}

	}
	for (int i = 0; i &lt; advertiseStrings.length; i++) {
		if (msg.contains(advertiseStrings[i])) {
			msg = msg.replace(advertiseStrings[i],
					getStars(advertiseStrings[i].length()));
		} else
			if (player.getRights() &gt;= 0)
				if (msg.contains(advertiseStrings[i])) {
					return null;

				}
	}
	return msg;
}


private static String getStars(int number) {
	String stars = "";
	for (int i = 0; i &lt; number; i++)
		stars += "*";
	return stars;
}
</code></pre>
<p>}<br>
[/code]</p>
          </blockquote>
