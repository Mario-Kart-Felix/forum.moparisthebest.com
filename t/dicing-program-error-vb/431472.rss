[/code]

From this code;

[code=vb]
Private Sub Button8_Click(sender As System.Object, e As System.EventArgs) Handles Button8.Click
        Dim ht As New Hashtable
        Dim rannum, i As Integer
        i = 1
        Do Until ht.Count = 1
            Randomize()
            rannum = Int(Rnd() * 100)
            If Not ht.ContainsValue(rannum) And rannum &gt; 1 Then
                ht.Add(i, rannum)
                i += 1
            End If
        Loop
        MsgBox(&quot;You have rolled a&quot;, TextBox8.Text = ht(1), &quot;on the Percentile Dice.&quot;)
    End Sub[/code]</description>
    
