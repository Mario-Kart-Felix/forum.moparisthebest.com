          <blockquote>
              <p><a class="mention" href="/u/echo">@echo</a> off<br>
color 77<br>
title AytonScape Client Compiler<br>
echo ========================<br>
echo ==Type ‘c’ for compile==<br>
echo ==  Type ‘r’ for run  ==<br>
echo ========================<br>
echo Type any other to close<br>
:def<br>
set /p compile=Option:<br>
if %compile%==c goto compile<br>
if %compile%==C goto compile<br>
if %compile%==r goto run<br>
if %compile%==R goto run<br>
:compile<br>
“%programfiles%\java\jdk1.6.0_24\bin\javac.exe” *.java<br>
pause<br>
goto def<br>
:run<br>
JAVA -Xmx500m EGUI<br>
pause</p>
          </blockquote>
