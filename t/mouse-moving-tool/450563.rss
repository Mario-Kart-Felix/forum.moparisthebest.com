      <item>
        <title>Mouse Moving Tool</title>
        <dc:creator><![CDATA[@whackatre Whackatre]]></dc:creator>
        <description><![CDATA[
          <p><a href="https://forum.moparisthebest.com/u/whackatre">@whackatre</a> wrote:</p>
          <blockquote>
              <p>Something like this?</p>
<pre><code class="lang-auto">
/**
 * RobotUtils.java
 */

package net.projectrainbow.misc;

import java.awt.AWTException;
import java.awt.Point;
import java.awt.PointerInfo;
import java.awt.Robot;
import java.awt.MouseInfo;
import java.awt.event.*;

public class RobotUtils
{
	/**
	 * Our robot instance for this class.
	 */
	private static Robot robot;
	
	public static void click(int x, int y)
	{
		try
		{
			robot = new Robot();
			robot.mouseMove(x,  y);
			robot.mousePress(16);
			robot.mouseRelease(16);
		}
		catch (AWTException e)
		{
			e.printStackTrace();
		}
	}
	
	public static void move(int x, int y)
	{
		try
		{
			robot = new Robot();
			robot.mouseMove(x, y);
		}
		catch (AWTException e)
		{
			debug("Error with mouse move!");
			e.printStackTrace();
		}
	}
	
	public static int getMouseX()
	{
		return MouseInfo.getPointerInfo().getLocation().x;
	}
	
	public static int getMouseY()
	{
		return MouseInfo.getPointerInfo().getLocation().y;
	}
}</code></pre>
          </blockquote>
          <p><a href="https://forum.moparisthebest.com/t/mouse-moving-tool/450563/2">Read full topic</a></p>
