                        addSkillXP((61512*playerLevel[0]), 0);		
                        addSkillXP((101333*playerLevel[1]), 3);	
                        actionTimer = 100; 
	               } 
                       else if(actionTimer =&gt; 1)  
                       {
              sendMessage("You can only click once!");              
                       } else { 
               sendMessage("something?");
             </code></p>
<p>that way he doesn’t have to add anything</p>
