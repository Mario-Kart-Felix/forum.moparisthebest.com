<p>Program boringdropperandchopper;<br>
{.include srl/srl.scar.}<br>
{.include srl/srl/skill/Woodcutting.scar}<br>
{.include SRL/SRL/Extended/xMapWalk.Scar}</p>
<p>const<br>
treecolor= 4616570;<br>
TreeTime = 30; //Time in seconds to wait inbetween trees.</p>
<p>//----------------------------------------------------------------------------\<br>
procedure drop;<br>
begin<br>
if (invcount=27)then<br>
DropAll;<br>
FindNormalRandoms;<br>
end;<br>
//----------------------------------------------------------------------------\<br>
procedure runaway;<br>
begin<br>
If (FindFight) then<br>
begin<br>
Runawaydirection(‘N’)<br>
Wait(25000+Random(5000))<br>
RunBack;<br>
end;<br>
end;<br>
//----------------------------------------------------------------------------\<br>
Procedure ChopWillows;<br>
var<br>
tmpT: Integer;</p>
<p>Begin<br>
If(IsUpTextMulti(‘down’,‘op dow’,‘chop’))Then<br>
begin<br>
Getmousepos(x,y)<br>
Mouse(x,y,0,0,True);<br>
End else<br>
repeat<br>
If(FindColorSpiralTolerance(x,y,treecolor,197,61,384,204,15))Or<br>
(FindColorSpiralTolerance(x,y,treecolor,197,61,384,204,15))Or<br>
(FindColorSpiralTolerance(x,y,treecolor,197,61,384,204,15))Then<br>
MMouse(x,y,3,3);<br>
Wait(500+Random(500));<br>
Until (IsUpTextMulti(‘down’,‘op dow’,‘chop’))<br>
If(IsUpTextMulti(‘down’,‘op dow’,‘chop’))Then<br>
begin<br>
GetMousePos(x, y);<br>
Mouse(x,y,0,0,True);<br>
end;<br>
MarkTime(TmpT);<br>
repeat<br>
RunAway;<br>
FindEnt(TreeColor);<br>
Wait(250);<br>
until(TimeFromMark(TmpT) &gt;= TreeTime);<br>
end;<br>
//----------------------------------------------------------------------------\<br>
//----------------------------------------------------------------------------\<br>
Procedure EntRun;<br>
Begin<br>
Case Random(3) Of<br>
1: Mouse(640,80,0,0,True);<br>
2: Mouse(650,91,0,0,true);<br>
3: Mouse(650,80,0,0,True);<br>
End;<br>
End;<br>
//----------------------------------------------------------------------------\<br>
begin<br>
ActivateClient;<br>
SetUpSRL;<br>
ClearDebug;<br>
FindNormalRandoms;<br>
Writeln(‘dropping your dam logs…remember to weild the axe!’);<br>
Writeln(‘And chopping the tree…thanks for using this…’);<br>
Writeln(‘This is my first auto chopper script thanks again for using this’);<br>
Repeat<br>
ChopWillows;<br>
drop;<br>
until(false);<br>
end.[/code]</p>
