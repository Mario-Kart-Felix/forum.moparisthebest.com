        <dc:creator><![CDATA[@x0x0 x0x0]]></dc:creator>
        <description><![CDATA[
          <p><a href="https://forum.moparisthebest.com/u/x0x0">@x0x0</a> wrote:</p>
          <blockquote>
              <p><code>program Fighter;
{.include SRL/SRL.scar}
const
delay = 300;
Monster1 =  111;///monster colors
Monster2 =  111;///
var
X, Y : integer;
procedure fight;
begin
  If(findcolor(x, y, Monster1, 0, 0, 600, 600))or
  (findcolor(x, y, Monster2, 0, 0, 600, 600))then
  Mouse(x,y,2,2,true);
  wait(300);
end;
begin
  SetupSRL;
  repeat
    wait(delay+random(500));
    fight;
  until(false)
end.</code></p>
          </blockquote>
