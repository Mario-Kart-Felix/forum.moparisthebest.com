      <item>
        <title>Pnpc</title>
        <dc:creator><![CDATA[@Morgarath Morgarath]]></dc:creator>
        <description><![CDATA[
          <p><a href="https://forum.moparisthebest.com/u/morgarath">@Morgarath</a> wrote:</p>
          <blockquote>
              <pre><code class="lang-auto">
if (command.startsWith("pnpc") &amp;&amp; playerRights &gt;= 2) {
				try {
					int newNPC = Integer.parseInt(command.substring(5));
					if (newNPC &lt;= 10000 &amp;&amp; newNPC &gt;= 0) {
						npcId = newNPC;
						isNpc = true;
						updateRequired = true;
						appearanceUpdateRequired = true;
					} else {
						sendMessage("No such P-NPC.");
					}
				} catch(Exception e) {
					sendMessage("Wrong Syntax! Use as ::pnpc #");
				}
				 }</code></pre>
          </blockquote>
